              <component id="4947" class="javax.swing.JButton" binding="btnForward_100">
                <constraints border-constraint="South"/>
                <properties>
                  <text value="⇓ 100"/>
                </properties>
              </component>
              <grid id="8a3fb" layout-manager="BorderLayout" hgap="0" vgap="0">
